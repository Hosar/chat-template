{
  "name": "chat-template",
  "author": "Seven Leaps",
  "version": "0.0.3",
  "description": "Simulating chats using React",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel ./src --ignore *.spec.js --out-dir ./build",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "coverage": "npm run test",
    "coverage:combine": "istanbul report --dir test/coverage/combined --include test/**/*coverage.json text-summary lcovonly json",
    "lint": "eslint src docs/src test/browser test/unit && echo \"eslint: no lint errors\"",
    "prebuild": "npm run clean:build",
    "storybook": "start-storybook -p 9001",
    "test": "npm run test:unit && npm run test:inte && npm run test:unit:coverage && npm run test:inte:coverage && npm run coverage:combine",
    "test:unit": "babel-node test/unit.js",
    "test:unit:watch": "babel-node test/unit.watch.js",
    "test:unit:coverage": "babel-node node_modules/.bin/isparta cover --report lcovonly --include-all-sources --dir test/coverage/unit test/unit.js",
    "test:inte": "babel-node test/integration.js",
    "test:inte:watch": "babel-node test/integration.watch.js",
    "test:inte:coverage": "babel-node node_modules/.bin/isparta cover --report lcovonly --include-all-sources --dir test/coverage/integration test/integration.js"
  },
  "keywords": [
    "react",
    "react-component",
    "chat-template"
  ],
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/seven-leaps/chat-template"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/seven-leaps/chat-template/issues"
  },
  "homepage": "",
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "dependencies": {
    "inline-style-prefixer": "^1.0.2",
    "keycode": "^2.1.0",
    "lodash.merge": "^4.1.0",
    "lodash.throttle": "^4.0.0",
    "react-addons-create-fragment": "^15.0.1",
    "react-addons-transition-group": "^15.0.1",
    "react-addons-update": "^15.0.1",
    "react-event-listener": "^0.1.1",
    "recompose": "^0.17.0",
    "simple-assign": "^0.1.0",
    "warning": "^2.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.10.1",
    "app-module-path": "^1.0.5",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.3.13",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.2.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.5.1",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.0.0",
    "fs-extra": "^0.26.7",
    "glob": "^7.0.0",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.9.1",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.9"
  }
}
