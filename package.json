{
  "name": "chat-template",
  "version": "0.0.22",
  "description": "Chat-Template is a React component that enables quick prototyping of bot conversations.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sevenleaps/chat-template.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "bundle-js-dev": "webpack --config ./webpack.dev.config.js",
    "bundle-js-prod": "webpack -p --config ./webpack.prod.config.js",
    "bundle-js": "npm run test && npm run bundle-js-dev && npm run bundle-js-prod"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.22.1",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "git-url-parse": "^6.0.2",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "raw-loader": "^0.5.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.2"
  },
  "dependencies": {
    "aphrodite": "^0.3.1",
    "object-assign-deep": "0.0.4",
    "material-ui": "0.15.0",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
